    "bundle": "react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios' --assets-dest='./ios'",
    "android": "adb reverse tcp:8081 tcp:8081 && react-native run-android",
    "a": "yarn android",
    "ios": "react-native run-ios",
    "ios:device": "react-native run-ios --device",
    "i:d": "yarn ios:device",
    "i": "yarn ios",
    "start": "react-native start > /dev/null",
    "l": "react-native link",
    "start:reset": "npx react-native start --reset-cache",
    "test": "jest",
    "lint": "eslint .",
    "fix": "sh startupScripts/index.sh && sh reactCoreFix.sh",
    "pod:install": "cd ios && pod install && cd ..",
    "p:i": "yarn pod:install",
    "pod:update": "cd ios && pod update && cd ..",
    "pod:reset": "cd ios && pod deintegrate && pod install && cd ..",
    "watchman:del": "watchman watch-del-all",
    "android:assemble": "rm -rf android/app/build/outputs/apk/release/*.apk && cd android && ./gradlew assembleRelease && cd ..",
    "a:a": "yarn jetify && yarn android:assemble",
    "android:clean": "cd android && ./gradlew clean && cd ..",
    "a:c": "yarn android:clean",
    "android:production": "npx react-native run-android --variant=release",
    "a:p": "yarn android:production",
    "android:install": "adb install -r -d android/app/build/outputs/apk/release/app-release.apk",
    "a:i": "yarn android:install",
    "cache:clear": "rm -rf /tmp/metro-bundler-cache-* && rm -rf /tmp/haste-map-react-native-packager-* && watchman watch-del-all && npm cache verify",
    "c:c": "yarn cache:clear",
    "postinstall": "patch-package"
  },
